#include <stdio.h>

int main(void) {
    FILE *in1 = fopen("input.txt", "r");
    FILE *in2 = fopen("input2.txt", "r");
    FILE *out = fopen("output.txt", "w");

    int num;
    int f1 = 0, f2 = 0; 


    if (in1) {
        for (int i = 0; i < 10; i++) {
            if (fscanf(in1, "%d", &num) != 1) {
                break; 
            } else if (num == 7) {
                f1 = 1;
                break;
            }
        }
        fclose(in1);
    }


    if (in2) {
        for (int i = 0; i < 10; i++) {
            if (fscanf(in2, "%d", &num) != 1) {
                break;
            } else if (num == 7) {
                f2 = 1;
                break;
            }
        }
        fclose(in2);
    }

    if (out) {
        if (f1 == 1) fprintf(out, "В первом: да\n");
        else fprintf(out, "В первом: нет\n");

        if (f2 == 1) fprintf(out, "Во втором: да\n");
        else fprintf(out, "Во втором: нет\n");

        fclose(out);
    }

    return 0;
}
