#include <stdio.h>
  
int main(void)
{
    FILE *input, *output;
    int a[10];
    int count = 0;
    input = fopen("input.txt","r");
    for (int i = 0; i<10; i++)
    {
        fscanf(input,"%d",&a[i]);
        if (a[i]>0 ||  a[i]<0)
        {
            count++;
        }
    }
    fclose(input);
    output = fopen("output.txt","w");
    fprintf(output,"%d",count);
    fclose(output);
    return 0;
}
